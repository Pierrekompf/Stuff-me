<!DOCTYPE html>
<html>

    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Stuff Me{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/styles.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/soon.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/fonts/font-awesome/css/font-awesome.min.css') }}" />
            <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <script src="{{ asset('assets/js/jquery-3.1.1.min.js') }}"></script>
    </head>

    <body>
        {% block header %}
        {% endblock %}

        {% block body %}

        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('assets/js/soon/jquery-1.11.3.min.js') }}"></script>
            <script src="{{ asset('assets/js/soon/jquery.backstretch.min.js') }}"></script>
            <script src="{{ asset('assets/js/soon/jquery.countdown.min.js') }}"></script>
            <script src="{{ asset('assets/js/soon/jquery.particleground.min.js') }}"></script>
            <script src="{{ asset('assets/js/soon/main.js') }}"></script>
            <script src="{{ asset('assets/js/soon/main_en.js') }}"></script>
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        {% endblock %}
    </body>
</html>
